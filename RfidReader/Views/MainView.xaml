                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <GroupBox Header="{DynamicResource SpParameters}" Grid.Row="0" FontSize="12">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Label Content="{DynamicResource LblBaudrate}" Grid.Column="0" FontSize="12" Margin="5, 0, 0, 0"/>
                                        <ComboBox Grid.Column="1" FontSize="12" ItemsSource="{Binding BaudRateList}" SelectedIndex="{Binding SpBaudrate}" Margin="5, 0, 0, 0"/>
                                        <Label Content="{DynamicResource LblDataBits}" Grid.Column="2" FontSize="12" Margin="10, 0, 0, 0"/>
                                        <ComboBox Grid.Column="3" FontSize="12" SelectedIndex="{Binding DataBits}" Margin="5, 0, 0, 0">
                                            <ComboBoxItem Content="5" FontSize="12"/>
                                            <ComboBoxItem Content="6" FontSize="12"/>
                                            <ComboBoxItem Content="7" FontSize="12"/>
                                            <ComboBoxItem Content="8" FontSize="12"/>
                                        </ComboBox>
                                        <Label Content="{DynamicResource LblStopBits}" Grid.Column="4" FontSize="12" Margin="10, 0, 0, 0"/>
                                        <ComboBox Grid.Column="5" FontSize="12" SelectedIndex="{Binding StopBits}" Margin="5, 0, 0, 0">
                                            <ComboBoxItem Content="1"/>
                                            <ComboBoxItem Content="2"/>
                                        </ComboBox>
                                        <Label Content="{DynamicResource LblCheckBits}" Grid.Column="6" FontSize="12" Margin="10, 0, 0, 0"/>
                                        <ComboBox Grid.Column="7" SelectedIndex="{Binding CheckBits}" Margin="5, 0, 0, 0">
                                            <ComboBoxItem Content="{DynamicResource ItemNone}"/>
                                            <ComboBoxItem Content="{DynamicResource ItemOdd}"/>
                                            <ComboBoxItem Content="{DynamicResource ItemEven}"/>
                                            <ComboBoxItem Content="{DynamicResource ItemMark}"/>
                                            <ComboBoxItem Content="{DynamicResource ItemClear}"/>
                                        </ComboBox>
                                    </Grid>
                                </GroupBox>
                                <GroupBox Header="{DynamicResource ModuleParameters}" Grid.Row="1" FontSize="12" Margin="0, 5, 0, 0">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="55"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Content="{DynamicResource LblDevMode}" Grid.Column="0" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <ComboBox Grid.Column="1" FontSize="12" SelectedIndex="{Binding DeviceMode}" Margin="5, 0, 0, 0">
                                                <ComboBoxItem Content="UDP Client"/>
                                                <ComboBoxItem Content="TCP Client"/>
                                                <ComboBoxItem Content="UDP Server"/>
                                                <ComboBoxItem Content="TCP Server"/>
                                            </ComboBox>
                                            <Label Content="{DynamicResource LblIpAddress}" Grid.Column="2" FontSize="12" Margin="10, 0, 0, 0"/>
                                            <TextBox Grid.Column="3" Text="{Binding SetIp}" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <Label Content="{DynamicResource Port}" Grid.Column="4" FontSize="12" Margin="10, 0, 0, 0"/>
                                            <TextBox Grid.Column="5" Text="{Binding SetPort}" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <Label Content="{DynamicResource LblSubNetMask}" Grid.Column="6" FontSize="12" Margin="10, 0, 0, 0"/>
                                            <TextBox Grid.Column="7" Text="{Binding SetSubNetMask}" FontSize="12" Margin="5, 0, 0, 0"/>
                                        </Grid>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Separator Grid.Column="0" VerticalAlignment="Center"/>
                                            <Label Grid.Column="1" Content="{DynamicResource StrDevParamsSetTips}" FontSize="10" Margin="5"/>
                                            <Separator Grid.Column="2" VerticalAlignment="Center"/>
                                        </Grid>
                                        <Grid Grid.Row="2" IsEnabled="{Binding IsTargetParamsEnabled}">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="55"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Content="{DynamicResource LblDestinationIp}" Grid.Column="0" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <TextBox Grid.Column="1" Text="{Binding SetTargetIp}" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <Label Content="{DynamicResource LblDestinationPort}" Grid.Column="2" FontSize="12" Margin="10, 0, 0, 0"/>
                                            <TextBox Grid.Column="3" Text="{Binding SetTargetPort}" FontSize="12" Margin="5, 0, 0, 0"/>
                                            <Label Content="{DynamicResource LblGateway}" Grid.Column="4" FontSize="12" Margin="10, 0, 0, 0"/>
                                            <TextBox Grid.Column="5" Text="{Binding SetGateway}" FontSize="12" Margin="5, 0, 0, 0"/>
                                        </Grid>
                                    </Grid>
                                </GroupBox>
                                <StackPanel Orientation="Horizontal" Grid.Row="2" Margin="0, 15, 0, 0"  HorizontalAlignment="Right" VerticalAlignment="Center">
                                    <Button Content="{DynamicResource DefaultParams}" Margin="5, 0, 5, 0" MinWidth="100" FontSize="14">
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxmvvm:EventToCommand EventName="Click" Command="{Binding GetSearchDeviceDefaultParamsCommand}"/>
                                        </dxmvvm:Interaction.Behaviors>
                                    </Button>
                                    <Button Content="{DynamicResource SetParams}" Margin="5, 0, 0, 0" MinWidth="100" FontSize="14">
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxmvvm:EventToCommand EventName="Click" Command="{Binding SetDeviceParamsCommand}"/>
                                        </dxmvvm:Interaction.Behaviors>
                                    </Button>
                                </StackPanel>
                            </Grid>
                        </GroupBox>
                    </Grid>
                </Grid>
            </dxwui:PageViewItem>
        </dxwui:PageView>
        <!--状态栏-->
        <dxb:StatusBarControl Height="30" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" BarItemsAlignment="Default" Margin="0,0.333,0.333,-0.333">
            <dxb:BarStaticItem x:Name="Version" Content="{Binding VersionInfo}"/>
            <dxb:BarStaticItem x:Name="Tip" AutoSizeMode="Fill" ContentAlignment="Right" Content="{Binding StatusTipInfo}"/>
        </dxb:StatusBarControl>
        <!--<dxga:StateIndicatorControl Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"/>-->
        <dx:WaitIndicator DeferedVisibility="{Binding IsShowLoading}" Content="Loading..." Grid.Row="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                          Grid.Column="0">
            <dx:WaitIndicator.ContentTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="{DynamicResource StrWaiting}" FontSize="20"/>
                        <TextBlock Text="{DynamicResource StrProcessing}"/>
                    </StackPanel>
                </DataTemplate>
            </dx:WaitIndicator.ContentTemplate>
        </dx:WaitIndicator>
    </Grid>
</UserControl>
